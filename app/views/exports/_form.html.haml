= form_for @export do |f|
  - if @export.errors.any?
    #error_explanation
      %h2= "#{pluralize(@export.errors.count, "error")} prohibited this export from being saved:"
      %ul
        - @export.errors.full_messages.each do |msg|
          %li= msg

  -# .field
    = f.label :complete
    = f.check_box :complete

  .actions
    = f.submit 'Save'
